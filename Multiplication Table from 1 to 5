package home_Assignments;

public class Table_1_to_5 {

	public static void main(String[] args) {	
		
		int mul=0;
		for(int i=1;i<=5;i++)
		{
			System.out.println("**** Table of "+i+"*****");
			for(int j=1;j<=10;j++)
			{
			mul=i*j;
			System.out.println(i +" * "+j+ " = " +mul);
			}			
		}		
	}

